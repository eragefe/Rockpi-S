// Definitions for DummyDAC
/dts-v1/;
/plugin/;

/ {
        compatible = "radxa,rockpis", "rockchip,rk3308";

        fragment@0 {
                target = <&i2s_8ch_0>;

                __overlay__ {
                        #sound-dai-cells = <0>;
                        status = "okay";
                        };
               };

        fragment@1 {
                target-path = "/";

                __overlay__ {
                        pcm5102a {
                                #sound-dai-cells = <0>;
                                status = "okay";
                                };
                        sound {
                                simple-audio-card,name = "SimpleDAC";
                                status = "okay";
                                };
                         };
               };
